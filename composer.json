{
    "name": "eighteen73/laravel-radioactivity",
    "description": "Laravel Radioactivity provides a lightweight trending system to your application.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Eighteen73\\Radioactivity\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eighteen73\\Radioactivity\\RadioactivityServiceProvider"
            ],
            "aliases": {
                "LaravelRating": "Eighteen73\\Radioactivity\\RadioactivityFacade"
            }
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/contracts": "^6.9|^7.0|^8.0",
        "illuminate/database": "^6.9|^7.0|^8.0",
        "illuminate/support": "^6.9|^7.0|^8.0"
    }
}
